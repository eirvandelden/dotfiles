[user]
  name = Etienne van Delden
  email = etienne@kabisa.nl
[alias]
  co = checkout
  ci = commit
[apply]
  whitespace = nowarn
[color]
  branch = auto
  diff = auto
  status = auto
[core]
  excludesfile = ~/.gitignore_global
[merge]
  ff = true
[github]
  user = eirvandelden
  token = !security 2>&1 >/dev/null find-generic-password -gs github.token | ruby -e 'print $1 if STDIN.gets =~ /^password: \\\"(.*)\\\"$/'
[push]
  default = simple
